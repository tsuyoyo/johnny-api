{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "removeComments": true,
    "plugins": [
      { "transform": "ts-auto-mock/transformer" }
    ]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./database/**/*",
    "./proto_out/**/*",
    "./test/**/*",
  ]
}
